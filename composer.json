{
    "name": "j6s/phparch",
    "description": "Architecture Testing for PHP projects",
    "license": "MIT",
    "authors": [
        {
            "name": "Johannes Hertenstein",
            "email": "phparch@thej6s.com"
        }
    ],
    "require": {
        "php": "^7.2",

        "nikic/php-parser": "~3.0|~4.0",
        "symfony/finder": "^3.4|^4.1",
        "phpdocumentor/reflection-docblock": "^4.3",
        "thecodingmachine/safe": "^0.1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "phpstan/phpstan-shim": "^0.10.6",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "suggest": {
        "phpunit/phpunit": "For assertion helpers to work correctly."
    },
    "autoload": {
        "psr-4": {
            "J6s\\PhpArch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "J6s\\PhpArch\\Tests\\": "tests/"
        }
    }
}
